<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="1">
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>

    <listeners>
        <listener class-name="Com.tutorialsninja.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Regression Test">
        <classes>
            <class name="Com.tutorialsninja.testsuite.DesktopsTest"></class>
            <class name="Com.tutorialsninja.testsuite.LaptopsAndNotebooksTest"></class>
            <class name="Com.tutorialsninja.testsuite.MyAccountTest"></class>
            <class name="Com.tutorialsninja.testsuite.TopMenuTest"></class>

        </classes>
    </test>
</suite>
